---
import '../global.css'

const {
	title = '',
	description = ''
} = Astro.props


---


<html lang='es' >
	<head>
		<meta charset='utf-8' />
		<meta name='viewport' content='width=device-width' />
		<title>{title}</title>
		<meta name='description' content={description} />

		<link rel='icon' href='/favicon.svg' type='image/svg+xml' />
	</head>

	<body ><slot /></body>
</html>